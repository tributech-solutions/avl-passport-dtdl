{
    "@context": "dtmi:dtdl:context;3",
    "@id": "dtmi:avl:passport:validation:rule:minmax;1",
    "@type": "Interface",
    "extends": "dtmi:avl:passport:validation:rule:base;1",
    "displayName": "Number Min/Max Validation Rule",
    "contents": [
        {
            "@type": "Property",
            "name": "Min",
            "schema": "double",
            "writable": true,
            "displayName": "Minimum Value",
            "description": "Property number is not allowed to be less than this value."
        },
        {
            "@type": "Property",
            "name": "Max",
            "schema": "double",
            "writable": true,
            "displayName": "Maximum Value",
            "description": "Property number is not allowed to be greater than this value."
        }
    ]
}